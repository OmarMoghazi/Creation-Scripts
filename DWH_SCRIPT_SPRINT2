CREATE TABLE DWH_DEV_DB.L_REASON_CODES(
REASON_ID			Integer		,
COD_REASON			Integer		,
TXT_DESC			String      ,
CTR_UPDAT_SRLNO		Integer 	,
COD_TASK			String      ,
MAKER_ID			String      ,
CHECKER_ID			String      ,
LAST_MNT_DT			Integer     ,
BUSINESS_DT			Integer
)CLUSTERED BY (COD_REASON) INTO 2 BUCKETS
STORED AS ORC
TBLPROPERTIES	('transactional'='true', "orc.compress"="SNAPPY")
;

CREATE TABLE DWH_DEV_DB.F_ACCOUNTS_LIMITS(
ACCT_LIMIT_ID			Integer,
CCY_ID					Integer,
LIMIT_NO				String ,
PARENT_LIMIT_NO			String ,
FLG_SECURED				String ,
LIMIT_AMT				DOUBLE ,
LIMIT_START_DT			Integer,
LIMIT_END_DT			Integer,
LIMIT_INTEREST_RAT		DOUBLE ,
LIMIT_VAR_ACCT			DOUBLE ,
LIMIT_VAR_OD			DOUBLE ,
EFFECTIVE_INEREST_RAT	DOUBLE ,
REPORTING_AMT			DOUBLE ,
AVAILABLE_AMT			DOUBLE ,
UTILISATION_AMT			DOUBLE ,
COLLATERAL_PCT			DOUBLE ,
UNCOLLECTED_AMT			DOUBLE ,
REMARKS					String ,
FLG_INTERNAL_FD			String ,
COD_INT_INDX			Integer,
REASON_ID				Integer,
CCY Restriction			String ,
LAST_NEW_UTIL_DATE		Integer,
AVAILABILITY_FLAG		String ,
COLLATERAL_CONTRIBUTION	DOUBLE ,
UNCOLLECTED_FUNDS_LIMIT	DOUBLE ,
FIRST_OD_DT				Integer,
LAST_OD_DT				Integer,
AMT_UTILIZED_TODAY		DOUBLE ,
AMT_REINSTATED_TODAY	DOUBLE ,
EXCESS_TENOR			DOUBLE ,
MATURED_UTIL			DOUBLE ,
RECORD_STAT				String ,
AUTH_STAT				String ,
MOD_NO					Integer,
ONCE_AUTH				String ,
LIAB_BR					String ,
MAKER_ID				Integer,
CHECKER_ID				Integer,
LAST_MNT_DT				Integer,
BUSINESS_DT				Integer
)CLUSTERED BY (LIMIT_NO) INTO 4 BUCKETS
STORED AS ORC
TBLPROPERTIES	('transactional'='true', "orc.compress"="SNAPPY")
;

CREATE TABLE DWH_DEV_DB.B_ACCOUNTS_LIMITS(
ACCT_ID					Integer,
ACCT_LIMIT_ID			Integer,
PARENT_LIMIT_ID			Integer,
BUSINESS_DT				Integer
)CLUSTERED BY (ACCT_ID , ACCT_LIMIT_ID) INTO 4 BUCKETS
STORED AS ORC
TBLPROPERTIES	('transactional'='true', "orc.compress"="SNAPPY")
;

CREATE TABLE DWH_DEV_DB.F_CARDS_LIMITS(
CARDS_LIMIT_ID			Integer,
CARD_ID					Integer,
CARD_LIM				String ,
CASH_LIM				String ,
SALE_LIM				String ,
BUSINESS_DT				Integer
)CLUSTERED BY (CARD_ID) INTO 4 BUCKETS
STORED AS ORC
TBLPROPERTIES	('transactional'='true', "orc.compress"="SNAPPY")
;

CREATE TABLE DWH_DEV_DB.D_CARD_TYPE_LIMITS(
CARD_TYPE_LIMITS_ID		Integer,
CARDTYPE_ID				Integer,
CASH_WD_LIM				DOUBLE ,
TC_LIM					DOUBLE ,
CASH_TC_LIM				DOUBLE ,
PURCH_LIMIT				DOUBLE ,
CREDIT_LIM				DOUBLE ,
TXNCODE					String ,
CASH_WD_COUNT			Integer,
TC_COUNT				Integer,
PURCH_COUNT				Integer,
CARD_LIFE				String ,
Business_DT				Integer
)CLUSTERED BY (CARDTYPE_ID) INTO 2 BUCKETS
STORED AS ORC
TBLPROPERTIES	('transactional'='true', "orc.compress"="SNAPPY")
